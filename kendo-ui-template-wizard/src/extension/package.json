{
  "name": "kendotemplatewizard",
  "displayName": "Kendo UI Template Wizard",
  "description": "This extension allows you to quickly scaffold apps built with KendoReact, Kendo UI for Angular or Kendo UI for Vue using a visual wizard.",
  "version": "1.1.1",
  "publisher": "KendoUI",
  "aiKey": "__AIKEY__",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#eff1f3",
    "theme": "light"
  },
  "icon": "resources/KendoUI_Template_Wizard.jpg",
  "keywords": [
    "Web Application",
    "Boilerplate",
    "React",
    "Angular",
    "Vue",
    "Scaffolding"
  ],
  "preview": true,
  "homepage": "https://github.com/telerik/kendo-vscode-extensions/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/telerik/kendo-vscode-extensions/issues"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-vscode-extensions",
    "vsce": "^1.57.1",
    "vscode-extension-telemetry": "^0.1.1",
    "vscode-nls": "^4.0.0"
  },
  "activationEvents": [
    "onCommand:kendoTemplateExtension.wizardLaunch"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kendoTemplateExtension.wizardLaunch",
        "title": "%kendoTemplateExtension.commands.wizardLaunch%",
        "category": "%kendoTemplateExtension.commands.wts%"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "%kendoTemplateExtension.commands.wts%",
      "properties": {
        "wts.defaultOutputPath": {
          "type": "string",
          "default": "",
          "description": "Load a default output path on wizard launch."
        },
        "wts.enablePreviewMode": {
          "type": "boolean",
          "default": false,
          "description": "Enable upcoming features; Warning: may cause stability issues."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "gulp compile",
    "build": "gulp build",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "generate": "node ./out/scripts/generate-test",
    "createVsixPackage": "gulp package"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.59",
    "del": "^4.1.1",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-filter": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "run-sequence": "^2.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "vscode": "^1.1.36",
    "vscode-nls-dev": "^3.3.1"
  },
  "extensionDependencies": [],
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "@types/app-root-path": "^1.2.4",
    "@types/archiver": "^2.1.3",
    "@types/fs-extra": "^5.1.0",
    "@types/node-fetch": "^2.5.4",
    "@types/request": "^2.48.3",
    "@types/rimraf": "^2.0.3",
    "app-root-path": "^2.2.1",
    "archiver": "^3.1.1",
    "azure-arm-cosmosdb": "^2.3.0",
    "azure-arm-resource": "^7.3.0",
    "azure-arm-website": "^5.7.0",
    "fs-extra": "^7.0.1",
    "global": "^4.4.0",
    "log4js": "^4.5.1",
    "ms-rest": "^2.5.3",
    "ms-rest-azure": "^2.6.0",
    "node-fetch": "^2.6.0",
    "open": "^7.0.0",
    "portfinder": "^1.0.25",
    "rimraf": "^2.7.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "url-parse": "^1.4.4",
    "vsce": "^1.71.0",
    "vscode-extension-telemetry": "^0.1.2",
    "vscode-nls": "^4.1.1"
  }
}
